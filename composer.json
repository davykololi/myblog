{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "based/momentum-modal": "^0.3.0",
        "cloudinary-labs/cloudinary-laravel": "^2.1",
        "cviebrock/eloquent-sluggable": "^11.0",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "esign/laravel-breadcrumbs": "^1.3",
        "glhd/gretel": "*",
        "inertiajs/inertia-laravel": "^1.0",
        "intervention/image": "^3.7",
        "jorenvanhocht/laravel-share": "^4.2",
        "kudashevs/laravel-share-buttons": "^5.1",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "robertboes/inertia-breadcrumbs": "^0.6.0",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-html": "^3.9",
        "spatie/laravel-newsletter": "^5.1",
        "spatie/laravel-permission": "*",
        "spatie/laravel-responsecache": "^7.5",
        "spatie/laravel-sitemap": "^7.2",
        "spatie/schema-org": "^3.23",
        "tightenco/ziggy": "^2.0",
        "tinymce/tinymce": "^7.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
